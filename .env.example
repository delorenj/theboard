# Database Configuration
POSTGRES_USER=theboard
POSTGRES_PASSWORD=theboard
POSTGRES_DB=theboard
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
DATABASE_URL=postgresql+psycopg://theboard:theboard@localhost:5432/theboard

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=whatever
REDIS_DB=0
REDIS_URL=redis://:whatever@localhost:6379/0

# Qdrant Configuration (uses existing native Qdrant on proxy network)
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_GRPC_PORT=6334
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=somethingsomething

# RabbitMQ Configuration
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_MANAGEMENT_PORT=15672
RABBITMQ_USER=theboard
RABBITMQ_PASSWORD=theboard
RABBITMQ_URL=amqp://theboard:theboard@localhost:5672/

# LLM Provider Configuration - OpenRouter
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Application Configuration
ENVIRONMENT=development
LOG_LEVEL=INFO
DEBUG=true

# Agent Configuration
MAX_ROUNDS=5
DEFAULT_STRATEGY=sequential
DEFAULT_AGENT_COUNT=5
CONVERGENCE_THRESHOLD=0.2
CONVERGENCE_ROUNDS=2
CONTEXT_SIZE_WARNING=15000
CONTEXT_SIZE_LIMIT=20000

# Compression Configuration
COMPRESSION_SIMILARITY_THRESHOLD=0.85
COMPRESSION_MIN_SUPPORT=2
LAZY_COMPRESSION_THRESHOLD=10000

# Embedding Configuration
EMBEDDING_MODEL=all-MiniLM-L6-v2
EMBEDDING_BATCH_SIZE=100
